<p id="notice"><%= notice %></p>

<h1>Listing Submitters</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone location</th>
      <th>Phone number</th>
      <th>Country</th>
      <th>Pen name first</th>
      <th>Pen name last</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submitters.each do |submitter| %>
      <tr>
        <td><%= submitter.first_name %></td>
        <td><%= submitter.last_name %></td>
        <td><%= submitter.email %></td>
        <td><%= submitter.phone_location_id %></td>
        <td><%= submitter.phone_number %></td>
        <td><%= submitter.country %></td>
        <td><%= submitter.pen_name_first %></td>
        <td><%= submitter.pen_name_last %></td>
        <td><%= link_to 'Show', submitter %></td>
        <td><%= link_to 'Edit', edit_submitter_path(submitter) %></td>
        <td><%= link_to 'Destroy', submitter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Submitter', new_submitter_path %>
